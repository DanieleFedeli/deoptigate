{
  "name": "deoptigate-app",
  "version": "0.0.0",
  "description": "Investigates v8/Node.js function deoptimizations in the browser.",
  "main": "main.js",
  "scripts": {
    "start": "budo ./main.js --css ./main.css -p 9922",
    "env": "env"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/thlorenz/deoptigate.git"
  },
  "homepage": "https://github.com/thlorenz/deoptigate",
  "keywords": [],
  "author": {
    "name": "Thorsten Lorenz",
    "email": "thlorenz@gmx.de",
    "url": "http://thlorenz.com"
  },
  "license": {
    "type": "MIT",
    "url": "https://github.com/thlorenz/deoptigate/blob/master/LICENSE"
  },
  "engine": {
    "node": ">=8"
  },
  "devDependencies": {
    "bubleify": "~1.2.0",
    "budo": "~11.2.1",
    "loose-envify": "~1.3.1",
    "react": "~16.3.2",
    "react-dom": "~16.3.2",
    "scroll-into-view-if-needed": "~2.2.2",
    "tachyons": "~4.9.1"
  },
  "browserify": {
    "transform": [
      [
        "bubleify",
        {
          "target": {
            "chrome": 63,
            "firefox": 58,
            "node": 8,
            "safari": 11,
            "edge": 16
          }
        }
      ]
    ]
  }
}
